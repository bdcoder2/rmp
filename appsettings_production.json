{
   /*
   C-style comments are allowed ...
   */
   "appsettings": {
      "section1": {
         "key1": "production_value1", // Double slash comments are allowed
         "key2": "production_value2"
      },
      "section2": {
         "key1": "production_value1",
         "key2": "production_value2"
      }
   },
   "Logging": {
      "LogLevel": {
         "Default": "Error"
      },
      "Debug": {
         "LogLevel": {
            "Default": "Error"
         }
      },
      "Console": {
         "LogLevel": {
            "Default": "Error",
            "Microsoft.Hosting.Lifetime": "Error",
            "Microsoft.AspNetCore.Routing": "Error",
            "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Error"
         }
      },
      "EventSource": {
         "LogLevel": {
            "Microsoft": "Error"
         }
      },
      "EventLog": {
         "LogLevel": {
            "Microsoft": "Error"
         }
      }
   },
   "AllowedHosts": "*"
}
